fun @main(): i32 {
%block0:
  @a_0 = alloc i32
  store 10, @a_0
  %0 = load @a_0
  %1 = eq %0, 1
  br %1, %block2, %block1

%block2:
  %2 = load @a_0
  %3 = eq %2, 2
  br %3, %block3, %block1

%block3:
  @b_0 = alloc i32
  store 2, @b_0
  jump %block1

%block1:
  @b_1 = alloc i32
  store 1, @b_1
  jump %block1

%block4:
  ret 0

}

